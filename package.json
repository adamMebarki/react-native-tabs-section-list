{
  "name": "react-native-tabs-section-list",
  "version": "2.0.0",
  "description": "React Native SectionList with scrollable tabs, fork of Artem Bogoslavskiy's react-native-tabs-section-list",
  "main": "src/index.ts",
  "repository": "https://github.com/adamMebarki/react-native-tabs-section-list.git",
  "author": "Artem Bogoslavskiy",
  "license": "MIT",
  "react-native": "src/index.ts",
  "types": "lib/typescript/index.d.ts",
  "scripts": {
    "eslint": "eslint --fix src/",
    "prettify": "yarn prettier --check \"src/**/*.ts*\"",
    "prepare": "bob build",
    "version": "git status --porcelain",
    "postversion": "git push --tags"
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "keywords": [
    "react-native",
    "react",
    "native",
    "android",
    "ios",
    "scrollview",
    "flatlist",
    "sectionlist",
    "scroll",
    "tabs"
  ],
  "homepage": "https://github.com/bogoslavskiy/react-native-tabs-section-list",
  "devDependencies": {
    "react": "16.13.1",
    "react-native": "0.63.4",
    "tslint": "5.12.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "~4.2.3",
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "@types/lodash": "~4.14.168",
    "@types/react": "16.9.50",
    "@types/react-native": "~0.63.52",
    "@typescript-eslint/eslint-plugin": "~4.18.0",
    "@typescript-eslint/eslint-plugin-tslint": "~4.18.0",
    "@typescript-eslint/parser": "~4.18.0",
    "babel-jest": "^24.8.0",
    "eslint": "~7.22.0",
    "eslint-plugin-prettier": "~3.3.1",
    "eslint-plugin-react": "~7.22.0",
    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-plugin-simple-import-sort": "~7.0.0",
    "eslint-plugin-unused-imports": "~1.1.0",
    "lodash": "~4.17.21",
    "prettier": "~2.2.1",
    "react-native-builder-bob": "~0.18.1",
    "tslib": "2.1.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "typescript",
      "module"
    ]
  },
  "peerDependencies": {
    "lodash": "~4.17.15",
    "react": "~16.13.1",
    "tslib": "~2.1.0",
    "react-native": "~0.63.4"
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
